| + setHour(int): void                  |
| + getHour(): int                      |
| + setMinute(int): void                |
| + getMinute(): int                    |
| + setSecond(int): void                |
| + getSecond(): int                    |
| + toUniversalString(): String         |
| + toStandardString(): String          |
